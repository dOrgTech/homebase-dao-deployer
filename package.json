{
  "name": "@dorgtech/homebase-dao-deployer",
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/index.ts",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "concurrently \"resolve-tspaths \" \"npx tsc --watch\" \"resolve-tspaths \" \"nodemon -q dist/index.js\" \"resolve-tspaths \"",
    "build": "tsc && resolve-tspaths",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint:ci": "eslint --color 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:check": "eslint --quiet 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@airgap/beacon-types": "^4.1.0",
    "@date-io/dayjs": "1.x",
    "@taquito/beacon-wallet": "^17.0.0",
    "@taquito/signer": "^17.0.0",
    "@taquito/taquito": "^17.0.0",
    "@taquito/tzip16": "^17.0.0",
    "axios": "^1.6.3",
    "bignumber.js": "^9.0.1",
    "blockies-ts": "^1.0.0",
    "body-parser": "^1.20.2",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "dayjs": "1.10.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-queue": "^0.0.13",
    "graphql": "^15.5.1",
    "graphql-request": "^3.4.0",
    "node-fetch": "2",
    "resolve-tspaths": "^0.8.17",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/connect-timeout": "^0.0.39",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "concurrently": "^8.2.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.2.0",
    "source-map-explorer": "2.5.2",
    "typescript": "^5.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn lint:check"
    ]
  }
}
